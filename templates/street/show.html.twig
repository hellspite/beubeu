{% extends 'base.html.twig' %}

{% block title %}{{ street.title }} - {{ parent() }}{% endblock %}

{% block content %}
<div class="container exhibit-container">
    <div class="exhibit-title"><h1>{{ street.title }}</h1></div>
    <div class="exhibit-image"><img src="{{ asset('images/' ~ street.image) }}" class="img-responsive" /></div>
    <div class="exhibit-desc">
        {{ street.description|raw }}
    </div>

</div>
{% endblock %}
